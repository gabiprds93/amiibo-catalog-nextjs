{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "import/order": "off",
    "import/no-duplicates": "off"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "overrides": [
    {
      "files": "**/*.+(ts|tsx)",
      "parserOptions": {
        "project": "./tsconfig.json"
      }
    }
  ]
}
